{
  "name": "lemric/sail",
  "description": "Docker files for running a basic Symfony application.",
  "license": "MIT",
  "keywords": [
    "symfony",
    "docker"
  ],
  "authors": [
    {
      "name": "Dominik Labudzinski",
      "email": "dominik@labudzinski.com",
      "homepage": "https://labudzinski.com"
    },
    {
      "name": "Lemric",
      "homepage": "https://lemric.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/yaml": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Lemric\\Sail\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lemric\\Sail\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "symfony/framework-bundle": "^6.0 || ^7.0",
    "symfony/process": "^6.0 || ^7.0",
    "symfony/test-pack": "^v1.1.0"
  },
  "bin": [
    "bin/lemric"
  ]
}